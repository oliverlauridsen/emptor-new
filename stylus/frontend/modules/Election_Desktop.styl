    }
    .fade-in {
      opacity: 0;
      animation: ease-in 1 forwards fadeIn;
      /* kalder på vores keyframe "fadeIn", easer den ind og gentager den 1 gang */
      /* Forwards = Efter animationen er færdig, så bliver vi på vores sidste keyframe */
      animation-duration: 1.5s;
      animation-delay: 0.5s;
    }
    /* STARTSCREEN - CHOOSE CATEGORY */
