    /* LOADING SCREEN */
    #loading_main_content {
      background: #063849;
      /* For browsers that do not support gradients */
      background: -webkit-linear-gradient(top left, #063849, #04212b);
      /* For Safari 5.1 to 6.0 */
      background: -o-linear-gradient(left bottom, #063849, #04212b);
      /* For Opera 11.1 to 12.0 */
      background: -moz-linear-gradient(right bottom, #063849, #04212b);
      /* For Firefox 3.6 to 15 */
      background: linear-gradient(to right bottom, #063849, #04212b);
      /* Standard syntax */
      height: 100vh;
      width: 100%;
    }
    #logo_choose {
      position: absolute;
      top: 0;
      bottom: 0;
      left: 0;
      right: 0;
      margin: auto;
      height: auto;
